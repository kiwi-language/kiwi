package enums

enum ProductKind {

    DEFAULT(0),
    VIRTUAL(1),
    HOTEL(2),
    ;
    priv val code: int

    init(code: int) {
        this.code = code
    }

    static fn fromCode(code: int) -> ProductKind {
        for (kind in values()) {
            if (kind.code == code) return kind
        }
        throw NullPointerException("Cannot find a ProductKind for code " + code)
    }

}

