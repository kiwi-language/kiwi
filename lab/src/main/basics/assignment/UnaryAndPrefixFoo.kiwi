package assignment

class UnaryAndPrefixFoo {

    static var value: int = 0

    static fn getAndIncrement() -> int {
        return value++
    }

    static fn incrementAndGet() -> int {
        return ++value
    }

    static fn getAndDecrement() -> int {
        return value--
    }

    static fn decrementAndGet() -> int {
        return --value
    }

}

