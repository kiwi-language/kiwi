class Product {
    private title string name;
    private child ProductKind kind;

    public Product(string name, ProductKind kind) {
        this.name = name;
        this.kind = kind;
    }

    public string getName() {
        return this.name;
    }

    public void setName(string name) {
        this.name = name;
    }

    public ProductKind getKind() {
        return this.kind;
    }

    public void setKind(ProductKind kind) {
        this.kind = kind;
    }

}

class ProductKind {
    private title readonly string name;

    public ProductKind(string name) {
        this.name = name;
    }

    public string getName() {
        return this.name;
    }

    public string __name__(string name, int ordinal) {
        return name;
    }

}