import java.util.List;
import java.util.ArrayList;

struct Node<T> {
    private string label;
    private T value;

    public Node(string label, T value) {
        this.label = label;
        this.value = value;
    }

    public string getLabel() {
        return this.label;
    }

    public T getValue() {
        return this.value;
    }

}

struct MyList<T> {

    private string label;
    private readonly ArrayList<Node<T>> nodes;

    public MyList(string label, List<Node<T>> nodes) {
        this.label = label;
        this.nodes = new ArrayList<Node<T>>(nodes);
    }

    public string getLabel() {
        return this.label;
    }

    public List<Node<T>> getNodes() {
        return unew ArrayList<Node<T>>(this.nodes);
    }

}