class Product {
    private title string name;
    private Product.Kind kind;

    public Product(string name, string kind) {
        this.name = name;
        this.kind = this.new Product.Kind(kind);
    }

    public string getName() {
        return this.name;
    }

    public void setName(string name) {
        this.name = name;
    }

    public Product.Kind getKind() {
        return this.kind;
    }

    public void setKind(Product.Kind kind) {
        this.kind = kind;
    }

    @Tag(1)
    class Kind {
        private title readonly string name;

        public Kind(string name) {
            this.name = name;
        }

        public string getName() {
            return this.name;
        }

        public string __name__(string name, int ordinal) {
            return name;
        }

    }


}
