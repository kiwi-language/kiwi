import java.lang.Iterable;
import java.util.List;

class Utils {

    public static int|null findGt(List<int> list, int value) {
        var lambda = (int v):boolean -> {
            return v > value;
        };
        return Utils.<int>find(list, lambda);
    }

    public static <T> T|null find(Iterable<[never, T]> iterable, (T) -> boolean test) {
        var it = iterable.iterator()!!;
        while (it.hasNext()) {
            T next = it.next()!!;
            if (test(next)) {
                return next;
            }
        }
        return null;
    }

}