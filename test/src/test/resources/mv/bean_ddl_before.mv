import org.metavm.api.Index;

@Component
class FooService {

    private static Index<boolean, FooService> allFlagsIdx;

    private static Index<boolean, FooService> __allFlagsIdx__() {
        return new Index<boolean, FooService>("allFlags", false, (FooService s) : boolean -> {
            return s.getAllFlags();
        });
    }

    private readonly boolean allFlags;

    public FooService() {
        this.allFlags = true;
    }

    public boolean getAllFlags() {
        return this.allFlags;
    }

}