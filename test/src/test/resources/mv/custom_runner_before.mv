class Field {

    private string name;
    private any|null value;

    public Field(string name) {
       this.name = name;
       this.value = null;
    }

    public string getName() {
       return this.name;
    }

    public void setName(string name) {
       this.name = name;
    }

    public any|null getValue() {
        return this.value;
    }

    public void setValue(any|null value) {
        this.value = value;
    }

}