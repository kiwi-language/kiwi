class Base1 {

    private int value1;

    public Base1(int value1) {
        this.value1 = value1;
    }

    public int getValue1() {
        return this.value1;
    }

}

class Base2 extends Base1 {

     private int value2;

     public Base2(int value1, int value2) {
         super(value1);
         this.value2 = value2;
     }

     public int getValue2() {
         return this.value2;
     }

}

class Derived extends Base2 {

    private int value3;

    public Derived(int value1, int value2, int value3) {
        super(value1, value2);
        this.value3 = value3;
    }

    public int getValue3() {
        return this.value3;
    }

}