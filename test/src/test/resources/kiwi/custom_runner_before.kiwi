class Field(priv var name: string) {

    priv var value: any? = null

    fn getName() -> string {
       return this.name
    }

    fn setName(name: string) {
       this.name = name
    }

    fn getValue() -> any|null {
        return this.value
    }

    fn setValue(v: any|null) {
        this.value = v
    }

}