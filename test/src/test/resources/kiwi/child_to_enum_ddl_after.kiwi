import java.lang.IllegalStateException

class Product {
    private var name: string
    private var kind: ProductKind

    init(name: string, kind: ProductKind) {
        this.name = name
        this.kind = kind
    }

    func getName() -> string {
        return this.name
    }

    func setName(name: string) {
        this.name = name
    }

    func getKind() -> ProductKind {
        return this.kind
    }

    func setKind(kind: ProductKind) {
        this.kind = kind
    }

}

@Tag(1)
enum ProductKind {

    DEFAULT,
    HOTEL,
    VIRTUAL

;


    private deleted var name: string

    init() {
    }

    static func __map__(kind: ProductKind) -> ProductKind {
        var i = 0
        var values = ProductKind.values()
        while (i < values.length) {
           var name = values[i].name()!!
           if(name.equals(kind.name)) {
                return values[i]
           }
           i = i + 1
        }
        throw new IllegalStateException(concat("Failed to find an enum constant for: ", kind.name))
    }

}