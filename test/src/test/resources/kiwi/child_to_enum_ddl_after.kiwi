class Product(
    var name: string,
    var kind: ProductKind
)

@Tag(1)
enum ProductKind {

    DEFAULT,
    HOTEL,
    VIRTUAL

;


    priv deleted var label: string

    static fn __map__(kind: ProductKind) -> ProductKind {
        var i = 0
        var values = ProductKind.values()
        while (i < values.length) {
           var name = values[i].name!!
           if(name.equals(kind.label)) {
                return values[i]
           }
           i = i + 1
        }
        throw Exception(concat("Failed to find an enum constant for: ", kind.name))
    }

}