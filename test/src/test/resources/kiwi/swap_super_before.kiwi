class Base1 {

    priv var value1: int

    init(value1: int) {
        this.value1 = value1
    }

    fn getValue1() -> int {
        return this.value1
    }

}

class Base2: Base1 {

     priv var value2: int

     init(value1: int, value2: int) {
         super.init(value1)
         this.value2 = value2
     }

     fn getValue2() -> int {
         return this.value2
     }

}

class Derived: Base2 {

    priv var value3: int

    init(value1: int, value2: int, value3: int) {
        super.init(value1, value2)
        this.value3 = value3
    }

    fn getValue3() -> int {
        return this.value3
    }

}