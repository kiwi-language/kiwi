class Product {
    priv var name: string
    priv var price: int
    priv var inventory: Inventory
    priv var manufacturer: string

    init(name: string, price: int, inventory: Inventory, manufacturer: string) {
        this.name = name
        this.price = price
        this.inventory = inventory
        this.manufacturer = manufacturer
    }

    fn getName() -> string {
        return this.name
    }

    fn setName(name: string) {
        this.name = name
    }

    fn getPrice() -> int {
        return this.price
    }

    fn setPrice(price: int) {
        this.price = price
    }

    fn getInventory() -> Inventory {
        return this.inventory
    }

    fn setInventory(inventory: Inventory) {
        this.inventory = inventory
    }

    fn getManufacturer() -> string {
        return this.manufacturer
    }

    fn setManufacturer(manufacturer: string) {
        this.manufacturer = manufacturer
    }

}

class Inventory {
    priv var quantity: int

    init(quantity: int) {
        this.quantity = quantity
    }

    fn getQuantity() -> int {
        return this.quantity
    }

    fn setQuantity(quantity: int) {
        this.quantity = quantity
    }

}

class Box<T> {
    priv var item: T

    init(item: T) {
        this.item = item
    }

    fn getItem() -> T {
        return this.item
    }

    fn setItem(item: T) {
        this.item = item
    }

}