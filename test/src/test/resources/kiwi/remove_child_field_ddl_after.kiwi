import java.util.List

class Product {
    private var name: string

    init(name: string) {
        this.name = name
    }

    func getName() -> string {
        return this.name
    }

    func setName(name: string) {
        this.name = name
    }

}

class Inventory {
    private var quantity: int

    init(quantity: int) {
        this.quantity = quantity
    }

    func getQuantity() -> int {
        return this.quantity
    }

    func setQuantity(quantity: int) {
        this.quantity = quantity
    }

}

class Box<T> {
    private val item: T

    init(item: T) {
        this.item = item
    }

    func getItem() -> T {
        return this.item
    }

}