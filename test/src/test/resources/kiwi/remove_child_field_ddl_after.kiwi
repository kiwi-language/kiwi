
class Product(priv var name: string) {

    fn getName() -> string {
        return this.name
    }

    fn setName(name: string) {
        this.name = name
    }

}

class Inventory(priv var quantity: int) {

    fn getQuantity() -> int {
        return this.quantity
    }

    fn setQuantity(quantity: int) {
        this.quantity = quantity
    }

}

class Box<T>(priv val item: T) {

    fn getItem() -> T {
        return this.item
    }

}