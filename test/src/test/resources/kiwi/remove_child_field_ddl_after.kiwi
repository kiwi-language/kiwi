import java.util.List

class Product {
    priv var name: string

    init(name: string) {
        this.name = name
    }

    fn getName() -> string {
        return this.name
    }

    fn setName(name: string) {
        this.name = name
    }

}

class Inventory {
    priv var quantity: int

    init(quantity: int) {
        this.quantity = quantity
    }

    fn getQuantity() -> int {
        return this.quantity
    }

    fn setQuantity(quantity: int) {
        this.quantity = quantity
    }

}

class Box<T> {
    priv val item: T

    init(item: T) {
        this.item = item
    }

    fn getItem() -> T {
        return this.item
    }

}