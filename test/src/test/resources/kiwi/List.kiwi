import java.util.List
import java.util.ArrayList

class Node<T> {
    priv var label: string
    priv var value: T

    init(label: string, value:T) {
        this.label = label
        this.value = value
    }

    fn getLabel() -> string {
        return this.label
    }

    fn getValue() -> T {
        return this.value
    }

}

class MyList<T> {

    priv var label: string
    priv val nodes: ArrayList<Node<T>>

    init(label: string, nodes: List<Node<T>>) {
        this.label = label
        this.nodes = ArrayList<Node<T>>(nodes)
    }

    fn getLabel() -> string {
        return this.label
    }

    fn getNodes() -> List<Node<T>> {
        return ArrayList<Node<T>>(this.nodes)
    }

}