import java.util.List
import java.util.ArrayList

class Node<T> {
    private var label: string
    private var value: T

    init(label: string, value:T) {
        this.label = label
        this.value = value
    }

    func getLabel() -> string {
        return this.label
    }

    func getValue() -> T {
        return this.value
    }

}

class MyList<T> {

    private var label: string
    private val nodes: ArrayList<Node<T>>

    init(label: string, nodes: List<Node<T>>) {
        this.label = label
        this.nodes = new ArrayList<Node<T>>(nodes)
    }

    func getLabel() -> string {
        return this.label
    }

    func getNodes() -> List<Node<T>> {
        return new ArrayList<Node<T>>(this.nodes)
    }

}