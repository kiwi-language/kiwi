class Node<T>(
    priv var label: string,
    priv var value: T
) {

    fn getLabel() -> string {
        return label
    }

    fn getValue() -> T {
        return value
    }

}

class MyList<T>(
    priv var label: string,
    priv val nodes: Node<T>[]
) {

    fn getLabel() -> string {
        return label
    }

    fn getNodes() -> Node<T>[] {
        return nodes
    }

}