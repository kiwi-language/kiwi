
class Product {

    priv var inventory: Product.Inventory

    init(quantity: int) {
        this.inventory = this.Inventory(quantity)
    }

    fn getInventory() -> Product.Inventory {
        return this.inventory
    }

    class Inventory {
        priv var quantity: int

        init(quantity: int) {
            this.quantity = quantity
        }

        fn getQuantity() -> int {
            return this.quantity
        }

    }

}