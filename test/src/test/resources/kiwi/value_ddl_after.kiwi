class Product {
    priv var name: string
    priv var price: Price

    init(name: string, price: Price) {
        this.name = name
        this.price = price
    }

    fn getName() -> string {
        return this.name
    }

    fn setName(name: string) {
        this.name = name
    }

    fn getPrice() -> Price {
        return this.price
    }

    fn setPrice(price: Price) {
        this.price = price
    }

}

value class Price {
    priv val amount: double
    priv val currency: Currency

    init(amount: double, currency: Currency) {
        this.amount = amount
        this.currency = currency
    }

    fn getAmount() -> double {
        return this.amount
    }

    fn getCurrency() -> Currency {
        return this.currency
    }

}

enum Currency {
    YUAN,
    DOLLAR,
    POUND

;

}