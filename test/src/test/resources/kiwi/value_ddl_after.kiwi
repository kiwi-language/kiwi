class Product(
    var name: string,
    var price: Price
) {

    fn getName() -> string {
        return this.name
    }

    fn setName(name: string) {
        this.name = name
    }

    fn getPrice() -> Price {
        return this.price
    }

    fn setPrice(price: Price) {
        this.price = price
    }

}

value class Price(
    val amount: double,
    val currency: Currency
) {

    fn getAmount() -> double {
        return this.amount
    }

    fn getCurrency() -> Currency {
        return this.currency
    }

}

enum Currency {
    YUAN,
    DOLLAR,
    POUND

;

}