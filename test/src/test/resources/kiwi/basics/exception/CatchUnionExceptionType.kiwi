package exception

class CatchUnionExceptionType {

    static val array: int[] = new int[] {1, 2, 3}

    static fn get(index: int) -> int {
        try {
            if (array == null)
                throw NullPointerException()
            if (index >= array.length)
                throw IndexOutOfBoundsException()
            return array[index]
        } catch (e: IndexOutOfBoundsException|NullPointerException) {
            return -1
        }
    }

}

class IndexOutOfBoundsException(): Exception("Index out of bounds") {}

class NullPointerException(): Exception("Null pointer") {}