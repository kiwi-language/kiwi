package anonymous_class

class StaticAnonymousClassFoo {

    static fn iterator() -> Iterator<string> {
        return new Iterator<string>() {

            fn hasNext() -> bool {
                return false
            }

            fn next() -> string {
                throw Exception("No such element")
            }

        }
    }

    static fn test() -> bool {
        return iterator().hasNext()
    }

}

interface Iterator<E> {

    fn next() -> E

    fn hasNext() -> bool

}
