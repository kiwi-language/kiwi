package lambda

class MethodCallWithinLambda {

    fn print(list: any[]) {
        forEach<any>(list, (e) -> print0(e))
    }

    priv fn forEach<E>(list: E[], action: ([E, any]) -> void) {
        for (e in list) {
            action(e)
        }
    }

    priv fn print0(o: any) {
        print(o)
    }

    static fn test() -> bool {
        MethodCallWithinLambda().print(new any[] {"a", "b", "c"})
        return true
    }

}

