
class Product(
    var name: string,
    var kind: ProductKind
)

enum ProductKind(val code: int) {

    DEFAULT(0),
    VIRTUAL(1),
    HOTEL(2)

;

    priv deleted var label: string?

    static fn __map__(kind: ProductKind) -> ProductKind {
        var values = ProductKind.values()
        var i = 0
        while (i < values.length) {
            var name = values[i].name!!
            if(equals(name, kind.label)) {
                return values[i]
            }
            i = i + 1
        }
        throw Exception(concat("Can not find an enum constant of for: ProductKind name: ", kind.name))
    }

}