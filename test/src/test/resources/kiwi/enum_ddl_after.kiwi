
class Product(
    var name: string,
    var kind: ProductKind
) {

    fn isDefaultKind() -> bool {
        return this.kind.isDefault()
    }

}

enum ProductKind(val code: int) {

    DEFAULT(0),
    HOTEL(1),
    VIRTUAL(2)

;

    priv deleted var label: string?

    fn isDefault() -> bool {
        return this == ProductKind.DEFAULT
    }

    static fn __map__(kind: ProductKind ) -> ProductKind {
        if(kind.ordinal != -1) {
            return kind
        }
        var values = ProductKind.values()
        var i = 0
        while (i < values.length) {
            var name = values[i].name!!
            if(equals(name, kind.label)) {
                return values[i]
            }
            i = i + 1
        }
        throw Exception(concat("Failed to map enum constant: ", kind.name))
    }

}