package index

import org.metavm.api.Index

class Foo {

    class Child(var name) {

        static var nameIdx = Index<string, Child>(true, c -> c.name)

    }


}

@Bean
class ChildService {

    fn findChildByName(name: string) -> Foo.Child? {
        return Foo.Child.nameIdx.getFirst(name)
    }

}