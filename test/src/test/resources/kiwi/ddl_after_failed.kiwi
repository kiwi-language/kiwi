class Product(
    var name: string,
    var price: int,
    var quantity: int,
    var description: string
) {

    fn getName() -> string {
        return this.name
    }

    fn setName(name: string) {
        this.name = name
    }

    fn getPrice() -> int {
        return this.price
    }

    fn setPrice(price: int) {
        this.price = price
    }

    fn getQuantity() -> int {
        return this.quantity
    }

    fn setQuantity(quantity: int) {
        this.quantity = quantity
    }

    fn getDescription() -> string {
        return this.description
    }

    fn setDescription(description: string) {
        this.description= description
    }
}

class Box<T>(var item: T) {

    fn getItem() -> T {
        return this.item
    }

    fn setItem(item: T) {
        this.item = item
    }

}