class Product {
    priv var name: string
    priv var price: int
    priv var quantity: int
    priv var description: string

    init(name: string, price: int, quantity: int, description: string) {
        this.name = name
        this.price = price
        this.quantity = quantity
        this.description = description
    }

    fn getName() -> string {
        return this.name
    }

    fn setName(name: string) {
        this.name = name
    }

    fn getPrice() -> int {
        return this.price
    }

    fn setPrice(price: int) {
        this.price = price
    }

    fn getQuantity() -> int {
        return this.quantity
    }

    fn setQuantity(quantity: int) {
        this.quantity = quantity
    }

    fn getDescription() -> string {
        return this.description
    }

    fn setDescription(description: string) {
        this.description= description
    }
}

class Box<T> {
    priv var item: T

    init(item: T) {
        this.item = item
    }

    fn getItem() -> T {
        return this.item
    }

    fn setItem(item: T) {
        this.item = item
    }

}