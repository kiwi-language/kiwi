import java.lang.IllegalStateException

class Product(
    var name: string,
    var kind: ProductKind
) {

    fn getName() -> string {
        return this.name
    }

    fn setName(name: string) {
        this.name = name
    }

    fn getKind() -> ProductKind {
        return this.kind
    }

    fn setKind(kind: ProductKind) {
        this.kind = kind
    }

}

enum ProductKind {

    DEFAULT,
    HOTEL,
    VIRTUAL

;

}