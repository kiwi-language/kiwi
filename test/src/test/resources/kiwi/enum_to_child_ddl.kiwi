import java.lang.IllegalStateException

class Product {
    private var name: string
    private var kind: ProductKind

    init(name: string, kind: ProductKind) {
        this.name = name
        this.kind = kind
    }

    func getName() -> string {
        return this.name
    }

    func setName(name: string) {
        this.name = name
    }

    func getKind() -> ProductKind {
        return this.kind
    }

    func setKind(kind: ProductKind) {
        this.kind = kind
    }

}

enum ProductKind {

    DEFAULT,
    HOTEL,
    VIRTUAL

;

    init() {
    }

}