class Product {

    priv var name: string
    priv var kind: ProductKind

    init(name: string, kind: ProductKind) {
        this.name = name
        this.kind = kind
    }

    fn getName() -> string {
        return this.name
    }

    fn setName(name: string) {
        this.name = name
    }

    fn getKind() -> ProductKind {
        return this.kind
    }

    fn setKind(kind: ProductKind) {
        this.kind = kind
    }

}

value class ProductKind {
    priv val code: int
    priv val name: string

    init(code: int, name: string) {
        this.code = code
        this.name = name
    }

    fn getCode() -> int {
        return this.code
    }

    fn getName() -> string {
        return this.name
    }

    priv fn __name__(name: string, ordinal: int) -> string {
        return name
    }

}