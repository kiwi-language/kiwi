class Product(
    var name: string,
    var kind: ProductKind
) {

    fn getName() -> string {
        return this.name
    }

    fn setName(name: string) {
        this.name = name
    }

    fn getKind() -> ProductKind {
        return this.kind
    }

    fn setKind(kind: ProductKind) {
        this.kind = kind
    }

}

value class ProductKind(
    val code: int,
    val name: string
) {

    fn getCode() -> int {
        return this.code
    }

    fn getName() -> string {
        return this.name
    }

    priv fn __name__() -> string {
        return "Unknown"
    }

}