class Product {

    priv var name: string
    priv var kind: ProductKind

    init(name: string, kind: ProductKind) {
        this.name = name
        this.kind = kind
    }

    fn getName() -> string {
        return this.name
    }

    fn setName(name: string) {
        this.name = name
    }

    fn getKind() -> ProductKind {
        return this.kind
    }

    fn setKind(kind: ProductKind) {
        this.kind = kind
    }

}

enum ProductKind {

    DEFAULT(0),
    VIRTUAL(1),
    HOTEL(2)

;

    priv val code: int

    init(code: int) {
        this.code = code
    }

    fn getCode() -> int {
        return this.code
    }

}