class Product(
    priv var name: string,
    priv var kind: ProductKind
) {

    fn getName() -> string {
        return this.name
    }

    fn setName(name: string) {
        this.name = name
    }

    fn getKind() -> ProductKind {
        return this.kind
    }

    fn setKind(kind: ProductKind) {
        this.kind = kind
    }

}

enum ProductKind(priv val code: int) {

    DEFAULT(0),
    VIRTUAL(1),
    HOTEL(2)

;
    fn getCode() -> int {
        return this.code
    }

}