class Product {
    priv var name: string
    priv var inventory: Inventory

    init(name: string, quantity: int) {
        this.name = name
        this.inventory = this.Inventory(quantity)
    }

    fn getName() -> string {
        return this.name
    }

    fn setName(name: string) {
        this.name = name
    }

    fn getInventory() -> Inventory {
        return this.inventory
    }

    fn setInventory(inventory: Inventory) {
        this.inventory = inventory
    }

}

class Inventory {
    priv var quantity: int

    init(quantity: int) {
        this.quantity = quantity
    }

    fn getQuantity() -> int {
        return this.quantity
    }

    fn setQuantity(quantity: int) {
        this.quantity = quantity
    }

}

class Box<T> {
    priv val item: T

    init(item: T) {
        this.item = item
    }

    fn getItem() -> T {
        return this.item
    }

}