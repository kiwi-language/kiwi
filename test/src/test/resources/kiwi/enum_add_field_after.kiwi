enum Currency {

    YUAN(0.14),
    DOLLAR(1.0),
    POUND(1.32)

;
    

    priv val rate: double

    init(rate: double) {
        this.rate = rate
    }

    fn getRate() -> double {
        return this.rate
    }

    priv fn __rate__() -> double {
        if(this == Currency.YUAN) {
            return 0.14
        }
        if(this == Currency.DOLLAR) {
            return 1.0
        }
        if(this == Currency.POUND) {
            return 1.32
        }
        return 0
    }

}