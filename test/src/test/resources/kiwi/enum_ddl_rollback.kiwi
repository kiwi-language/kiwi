import java.lang.Enum

class Product {
    priv var name: string
    priv var kind: ProductKind

    init(name: string, kind: ProductKind) {
        this.name = name
        this.kind = kind
    }

    fn getName() -> string {
        return this.name
    }

    fn setName(name: string) {
        this.name = name
    }

    fn getKind() -> ProductKind {
        return this.kind
    }

    fn setKind(kind: ProductKind) {
        this.kind = kind
    }

}

class ProductKind {
    priv val name: string
    priv val code: int

    init(name: string, code: int) {
        this.name = name
        this.code = code
    }

    fn getName() -> string {
        return this.name
    }

    fn getCode() -> int {
        return this.code
    }

    fn __name__(name: string, ordinal: int) -> string {
        return name
    }

}