class Product {
    priv var name: string
    priv var  kind: Product.Kind

    init(name: string, kind: string) {
        this.name = name
        this.kind = this.Product.Kind(kind)
    }

    fn getName() -> string {
        return this.name
    }

    fn setName(name: string) {
        this.name = name
    }

    fn getKind() -> Product.Kind {
        return this.kind
    }

    fn setKind(kind: Product.Kind) {
        this.kind = kind
    }

    @Tag(1)
    class Kind {
        priv val name: string

        init(name: string) {
            this.name = name
        }

        fn getName() -> string {
            return this.name
        }

        fn __name__(name: string, ordinal: int) -> string {
            return name
        }

    }


}
