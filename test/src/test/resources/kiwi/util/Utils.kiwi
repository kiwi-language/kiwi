package util

class Utils {


    static fn test(list: string[], tests: string[]) -> bool {
       return Utils.containsAny<string>(list, tests)
    }

    static fn test2(list: string[], s1: string, s2: string) -> bool {
        return Utils.containsAny<string>(list, s1, s2)
    }

    static fn containsAny<T>(array: T[], values: T[]) -> bool {
        for (e1 in array) {
            for (e2 in values) {
                if (equals(e1, e2))
                    return true
            }
        }
        return false
    }

    static fn containsAny<T>(iterable: T[], value1: T, value2: T) -> bool {
        var list = new T[]
        list.append(value1)
        list.append(value2)
        return Utils.containsAny<T>(iterable, list)
    }

}