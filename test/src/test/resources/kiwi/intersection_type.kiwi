
interface Greet {

    fn greet(name: string) -> string

}

class Base {

    var name: string

    init(name: string) {
        this.name = name
    }

}

class Sub: Base, Greet {

    init(name: string) {
        super.init(name)
    }

    fn greet(name: string) -> string {
        return concat("Hello ", name)
    }

}

class Lab {

    static fn test() -> string {
        return greet(Sub("Kiwi"))
    }

    static fn greet(o: Base & Greet) -> string {
        return o.greet(o.name)
    }

}