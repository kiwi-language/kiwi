import java.util.List
import java.util.ArrayList

class Child {
    priv var name: string
    priv var next: Child?

    init(name: string) {
        this.name = name
        this.next = null
    }

    fn getName() -> string {
        return this.name
    }

    fn getNext() -> Child? {
        return this.next
    }

    fn setNext(next: Child?) {
        this.next = next
    }

}

class Parent {

    priv val children = ArrayList<Child>()

    init() {
        this.children.add(Child("child001"))
    }

    fn getChildren() -> List<Child> {
        return ArrayList<Child>(this.children)
    }

    fn test() {
        var child1 = this.children.getFirst()
        var child2 = Child("Child002")
        child2.setNext(child1)
        this.children.add(child2)
        this.children.clear()
    }

}